{
  "name": "bhcosta90/entity",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Costa\\Entity\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "authors": [
    {
      "name": "Bruno Costa",
      "email": "bhcosta90@gmail.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "ramsey/uuid": "^4.7",
    "nesbot/carbon": "^2.71",
    "rakit/validation": "^1.4"
  },
  "require-dev": {
    "pestphp/pest": "^2.19",
    "symfony/var-dumper": "^6.3",
    "captainhook/captainhook": "^5.18"
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "scripts": {
    "post-autoload-dump": "vendor/bin/captainhook install -f -s"
  }
}
